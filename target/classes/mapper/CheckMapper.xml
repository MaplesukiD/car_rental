<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cqut.keshe3.mapper.CheckMapper">

    <resultMap id="BaseResultMap" type="cqut.keshe3.domain.Check">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="checkDate" column="check_date" jdbcType="TIMESTAMP"/>
            <result property="carId" column="car_id" jdbcType="INTEGER"/>
            <result property="error" column="error" jdbcType="VARCHAR"/>
            <result property="score" column="score" jdbcType="INTEGER"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="orderId" column="order_id" jdbcType="INTEGER"/>
            <result property="status" column="status" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,check_date,car_id,
        error,score,user_id,
        order_id,status
    </sql>
</mapper>
